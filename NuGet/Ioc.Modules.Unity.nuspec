<?xml version="1.0"?>
<package >
  <metadata>
    <id>Ioc.Modules.Unity</id>
    <version>1.4.2</version>
    <title>Ioc.Modules.Unity</title>
    <authors>Martin Halliday</authors>
    <owners>Martin Halliday</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Configures packages using Ioc.Modules into a Unity container</description>
    <copyright>Copyright Martin Halliday 2014-2020</copyright>
    <releaseNotes>Updated to Unity 5</releaseNotes>
    <summary>
      Probes assemblies for IoC dependencies declared with Ioc.Modules and
      registers those dependencies with the Unity container.
    </summary>
    <projectUrl>https://github.com/Bikeman868/Ioc.Modules</projectUrl>
    <license type="expression">Apache-2.0</license>
    <tags>IOC modular package library Unity</tags>
    <dependencies>
      <group targetFramework="net40">
        <dependency id="CommonServiceLocator" version="1.0" />
        <dependency id="Unity" version="5.11.3" />
        <dependency id="Unity.Container" version="5.11.5" />
        <dependency id="Ioc.Modules.Contract" version="1.4.1" />
        <dependency id="Ioc.Modules" version="1.4.1" />
      </group>
      <group targetFramework="net45">
        <dependency id="CommonServiceLocator" version="1.0" />
        <dependency id="Unity" version="5.11.3" />
        <dependency id="Unity.Container" version="5.11.5" />
        <dependency id="Ioc.Modules.Contract" version="1.4.1" />
        <dependency id="Ioc.Modules" version="1.4.1" />
      </group>
    </dependencies>
    <references>
    </references>
  </metadata>
  <files>
    <file src="..\Ioc.Modules.Unity\bin\release\net40\Ioc.Modules.Unity.dll" target="lib\net40"/>
    <file src="..\Ioc.Modules.Unity\bin\release\net45\Ioc.Modules.Unity.dll" target="lib\net45"/>
    <file src="..\*.md" target=""/>
  </files>
</package>