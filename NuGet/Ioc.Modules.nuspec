<?xml version="1.0"?>
<package >
  <metadata>
    <id>Ioc.Modules</id>
    <version>1.4.1</version>
    <title>Ioc.Modules</title>
    <authors>Martin Halliday</authors>
    <owners>Martin Halliday</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>A modular way of configuring IOC dependencies that is independent of the container</description>
    <copyright>Copyright Martin Halliday 2014-2019</copyright>
    <releaseNotes>Converted projects in the solution to use new MSBuild format and split out .Net Standard compatible package</releaseNotes>
    <summary>
      Provides a mechanism for library authors to inform the hosting application
      about its IoC requirements. Applications can also use this internally to
      make their IoC configuration more modular.
    </summary>
    <projectUrl>https://github.com/Bikeman868/Ioc.Modules</projectUrl>
    <license type="expression">Apache-2.0</license>
    <tags>IOC modular package library</tags>
    <dependencies>
      <group targetFramework="net40">
        <dependency id="Ioc.Modules.Contract" version="1.4.1" />
      </group>
      <group targetFramework="net45">
        <dependency id="Ioc.Modules.Contract" version="1.4.1" />
      </group>
    </dependencies>
    <references>
    </references>
  </metadata>
  <files>
    <file src="..\Ioc.Modules\bin\release\net40\Ioc.Modules.dll" target="lib\net40"/>
    <file src="..\Ioc.Modules\bin\release\net45\Ioc.Modules.dll" target="lib\net45"/>
    <file src="..\*.md" target=""/>
  </files>
</package>